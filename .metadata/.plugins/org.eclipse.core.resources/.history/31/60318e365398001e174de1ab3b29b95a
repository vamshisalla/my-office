package corejava;

public class Constructors {
String name;
	public static void main(String[] args) {
	Constructors ram1 = new Constructors();	
	ram1.name = "vamshi";
	System.out.println(ram1.name);
	Constructors ram2 = new Constructors();
	ram2.name = "ranjith";
	System.out.println(ram2.name);
	Constructors ram3 = new Constructors();
	ram3.name = "krishna";
	System.out.println(ram3.name);
	//}
	
	}
}

